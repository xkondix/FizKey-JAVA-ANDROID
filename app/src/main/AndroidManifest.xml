            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Menu.Kinematyka.KinematykaMenuActivity"
            android:label="@string/menuKinematyka"
            android:parentActivityName=".Menu.MenuGlowneActivity" />
        <activity
            android:name=".Menu.Kinematyka.Rzuty.RzutPionowyActivity"
            android:label="@string/rzuty"
            android:parentActivityName=".Menu.Kinematyka.KinematykaMenuActivity" />
        <activity android:name=".MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>