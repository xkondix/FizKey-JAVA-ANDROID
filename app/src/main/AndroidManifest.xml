<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.konradkowalczyk.fizkey_java_android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Menu.Kinematyka.Rzuty.RzutPionowySymulacjaActivity"></activity>
        <activity android:name=".Menu.Kinematyka.Rzuty.RzutPionowyWykresActivity" />
        <activity
            android:name=".Menu.MenuGlowneActivity"
