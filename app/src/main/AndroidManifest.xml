<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.konradkowalczyk.fizkey_java_android">

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".menu.kinematyka.projection.horizontal.HorizonatalProjectionPlotActivity"
            android:screenOrientation="portrait" ></activity>
        <activity android:name=".menu.kinematyka.projection.horizontal.HorizonatalProjectionSimulationActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".menu.kinematyka.projection.horizontal.HorizonatalProjectionActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".menu.kinematyka.projection.oblique.ObliqueProjectionSimulationActivity"
            android:screenOrientation="landscape" />
        <activity android:name=".menu.kinematyka.projection.oblique.ObliqueProjectionPlotActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".menu.kinematyka.projection.oblique.ObliqueProjectionActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".quizzes.firebase.view.group.page_group.GroupActivity"
            android:label="@string/title_activity_group"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".quizzes.firebase.view.custom_quiz.CustomResultActivity"
            android:label="@string/title_activity_custom_result"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".quizzes.firebase.view.custom_quiz.CreateCustomQuizActivity" />
        <activity
            android:name=".quizzes.quizy.QuizActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".quizzes.QuizMenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".menu.kinematyka.projection.vertical.VerticalProjectionSimulationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".menu.kinematyka.projection.vertical.VerticalProjectionPlotActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".menu.MenuGlowneActivity"
            android:label="@string/menuGlowne"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".menu.kinematyka.KinematykaMenuActivity"
            android:label="@string/menuKinematyka"
            android:parentActivityName=".menu.MenuGlowneActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".menu.kinematyka.projection.vertical.VerticalProjectionActivity"
            android:label="@string/rzuty"
            android:parentActivityName=".menu.kinematyka.KinematykaMenuActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>